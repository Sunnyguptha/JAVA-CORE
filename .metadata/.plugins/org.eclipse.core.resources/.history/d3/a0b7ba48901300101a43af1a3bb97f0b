package com.List_Collection.java;

import java.util.ArrayList;
import java.util.Arrays;
import java.util.LinkedList;

public class LinkedListPractice_01 {

	public static void main(String[] args) {
		ArrayList<String> al=new ArrayList<>();
		al.add("s1");
		al.add("s2");
		al.add("s3");
		LinkedList<String> ll=new LinkedList<>();
		
		ll.add("Y1");
		ll.offer("Y2");
		ll.add("YY");
		ll.addAll(0,al);
		System.out.println("Befor element="+ll);
		
		//ll.remove("YY");
		//ll.clear();
		
		//ll.removeAll(al);
		
		//ll.set(5,"Y3");
		boolean b=ll.contains("YY");
		System.out.println(b);
		System.out.println("After element="+ll);
		
	}

}
